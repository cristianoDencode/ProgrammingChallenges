{
  "autoload": {
    "psr-4": {
        "APP\\": "StrategyPattern/"
    },
    "exclude-from-classmap": [
      "StrategyPattern/Problem/index.php"
    ]
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.9",
    "friendsofphp/php-cs-fixer": "^3.60"
  },
  "scripts": {
    "lint": "composer dump-autoload -o --strict-psr",
    "fix": [
      "php-cs-fixer fix StrategyPattern"
    ]
  }
}
