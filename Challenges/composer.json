{
  "config": {
        "platform": {
            "php": "8.5.2"
        }
  },
  "autoload": {
    "psr-4": {
        "APP\\": "StrategyPattern/",
        "Observer\\": "Observer/"
    },
    "exclude-from-classmap": [
      "StrategyPattern/Problem/index.php",
      "Observer/Problem/index.php"
    ]
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.9",
    "friendsofphp/php-cs-fixer": "^3.60"
  },
  "scripts": {
    "lint": "composer dump-autoload -o --strict-psr",
    "fix": [
      "php-cs-fixer fix StrategyPattern",
      "php-cs-fixer fix Observer"
    ]
  }
}
